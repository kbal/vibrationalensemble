UNITS LENGTH=A TIME=fs ENERGY=kcal/mol

DISTANCE ATOMS=1,2 LABEL=d1

HISTOGRAM ...
  ARG=d1
  GRID_MIN=1.0
  GRID_MAX=3.0
  GRID_BIN=800
  BANDWIDTH=0.01
  LABEL=hh
... HISTOGRAM

CONVERT_TO_FES GRID=hh TEMP=300 LABEL=ff
DUMPGRID GRID=hh FILE=histo STRIDE=20000
DUMPGRID GRID=ff FILE=fes STRIDE=20000

FLUSH STRIDE=200
PRINT ARG=d1 STRIDE=200 FILE=colvar


